{
  "mcpServers": {
    "fetch": {
      "command": "uvx",
      "args": [
        "mcp-server-fetch"
      ],
      "env": {
        "PYTHONIOENCODING": "utf-8"
      },
      "alwaysAllow": [
        "fetch"
      ]
    },
    "MCP Docs": {
      "command": "npx",
      "args": [
        "mcp-remote",
        "https://gitmcp.io/docs"
      ]
    }
  }
}